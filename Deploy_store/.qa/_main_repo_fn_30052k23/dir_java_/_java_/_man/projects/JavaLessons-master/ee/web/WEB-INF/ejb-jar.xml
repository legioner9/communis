<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
         version="3.1">
    <enterprise-beans>
        <session>
            <ejb-name>XmlBean</ejb-name>
            <local-bean/>
            <ejb-class>ee.ejb.beans.XmlBean</ejb-class>
            <session-type>Stateless</session-type>

            <env-entry>
                <env-entry-name>currencyEntry</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>Dollars</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>currencyRate</env-entry-name>
                <env-entry-type>java.lang.Double</env-entry-type>
                <env-entry-value>0.90</env-entry-value>
            </env-entry>
        </session>
    </enterprise-beans>
</ejb-jar>